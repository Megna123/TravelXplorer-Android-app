<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.androidbroadcast.reciever"
    android:versionCode="1"
    android:versionName="1.0.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:debuggable="true"
        android:icon="@drawable/icon"
        android:label="TravelXplorer"
        android:testOnly="true" >
        <activity
            android:name="com.androidbroadcast.reciever.SignInActivity"
            android:label="TravelXplorer" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.androidbroadcast.reciever.GooglePlacesExample"
            android:label="TravelXplorer" >
        </activity>
        <activity
            android:name="com.androidbroadcast.reciever.PlaceDetail"
            android:label="TravelXplorer" >
        </activity>
        <activity
            android:name="com.androidbroadcast.reciever.SignUp"
            android:label="TravelXplorer " >
        </activity>

        <provider
            android:name="com.android.tools.ir.server.InstantRunContentProvider"
            android:authorities="com.androidbroadcast.reciever.com.android.tools.ir.server.InstantRunContentProvider"
            android:multiprocess="true" />
    </application>

</manifest>